double w = 5.1;
double x = 2.3;
double y;
double z = - 3.32;
int a1 = 3;
int a2;
int a3 = 4;
y = w/x - z;
cout<<"b) y = "<<y<<endl;
y = (w-z)/x;
cout<<"b) y = "<<y<<endl;
y = w*x/z;
cout<<"c) y = "<<y<<endl;
y = w/x*z/a1;
cout<<"d) y = "<<y<<endl;
a2 = w*x;
cout<<"e) a2 = "<<a2<<endl;
a2 = x/a3+z;
cout<<"f) a2 = "<<a2<<endl;
a2 = a1%a3;
cout<<"g) a2 = "<<a2<<endl;
y = y/a3/w;
cout<<"h) y = "<<y<<endl;
y = (x+w)/w + x/(w+y)*a3;
cout<<"i) y= "<<y<<endl;
